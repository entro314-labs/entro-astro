{
  "name": "@entro314labs/entro-astro",
  "version": "1.0.0",
  "description": "Astro integration for Entrolytics - First-party growth analytics for the edge",
  "keywords": [
    "entrolytics",
    "analytics",
    "astro",
    "astro-integration",
    "web-analytics",
    "tracking"
  ],
  "author": "Entro314 Labs <hey@entrolytics.click>",
  "license": "MIT",
  "homepage": "https://entrolytics.click",
  "repository": {
    "type": "git",
    "url": "https://github.com/entro314-labs/entro-astro",
    "directory": "entro-astro"
  },
  "bugs": {
    "url": "https://github.com/entro314labs/entro-astro/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=22.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "astro": "^5.16.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "astro": ">=5.0.0"
  }
}
